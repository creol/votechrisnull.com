# Redirect /donate to Anedot donation page
Redirect 301 /donate https://secure.anedot.com/friends-of-chris-null/ccd5

# Enable serving .html files without extension
RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^(.*)$ $1.html [L]
